package  {		import flash.display.MovieClip;	import flash.events.MouseEvent;		public class EntryButton extends MovieClip {				const SPADES:Number = 1;      // List	    const CLUBS:Number = 2;       // Detelina	    const HEARTS:Number = 3;      // Srce	    const DIAMONDS:Number = 4;    // Karo	    const JUMPER:Number = 5;      // Skocko    	const STAR:Number = 6;        // Zvezda    	const ONE:Number = 7;         // One    	const TWO:Number = 8;         // Two    	const THREE:Number = 9;       // Three    	const FOUR:Number = 10;       // Four    	const ZERO:Number = 11;       // Zero        var sign:Number;		public function EntryButton() {			// constructor code            this.addEventListener(MouseEvent.MOUSE_DOWN,clicked);     		}		function clicked(event:MouseEvent) {						trace("In entrybutton clicked method");			var main:Main = (Main)(parent);			main.buttonClicked(this.sign);		}		public function setSign(signPar:Number) {			this.sign = signPar;			this.gotoAndStop(signPar);		}	}}