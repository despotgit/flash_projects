package  {		import flash.display.MovieClip;			public class Indicator extends MovieClip {				public function Indicator() {			// constructor code			//stop();		}				public function changeColor() {						//trace("in changeColor of Indicator");			trace("currentFrame is: " + currentFrame);									x++;						switch(currentFrame) {				case 1: trace("In frame 1 branch");				this.gotoAndStop(2);break;								case 2: trace("In frame 2 branch");				this.gotoAndStop(3);break;								case 3: trace("In frame 3 branch");				this.gotoAndStop(1);break;							}					}					}	}